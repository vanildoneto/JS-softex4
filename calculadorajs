var num1 = prompt('Digite o primeiro número');
var num2 = prompt('Digiteo segundo número');
operação=prompt('Digite a operação')

switch(operação) {

case "soma"
alert:("Resultado:" num1+num2)
break

case "subtração"
alert:("Resultado:" num1-num2)
break

case ("multiplicação")
alert:("Resultado:" num1*num2)
break

case ("divisão")
alert:("Resultado:" num1/num2)
break
    
default:alert("número invalido") 

}
